<div style="height: 20px;"></div>
<div class="blDataRow">
  <div class="blDataBlock">
    <div class="blInputLabel">Shipper Ref No</div>
    <div class="blInputText"><input type="text" pInputText [(ngModel)]="customerName" [disabled]="disabled"/></div>
  </div>
  <div class="blExtraSpace">&nbsp;</div>
  <div class="blDataBlock">
    <div class="blInputLabel">Export Job No</div>
    <div class="blInputText"><input type="text" pInputText [(ngModel)]="customerName" [disabled]="disabled"/></div>
  </div>
  <div class="blExtraSpace">&nbsp;</div>
  <div class="blDataBlock">
    <div class="blInputLabel">System</div>
    <div class="blInputText"><input type="text" pInputText [(ngModel)]="customerName" [disabled]="disabled"/></div>
  </div>
  <div class="blExtraSpace">&nbsp;</div>
  <div class="blDataBlock">
    <div class="blInputLabel">Last Web Release</div>
    <div class="blInputText"><input type="text" pInputText [(ngModel)]="customerName" [disabled]="disabled"/></div>
  </div>
</div>

<div style="height: 50px; border-bottom: 1px solid rgb(216, 216, 216);">
  <div class="blBottomSubBlock1" style="margin-right: 10px;">
    <div class="blBottomBtnBlock">View Web Doc. Set</div>
  </div>
  <div class="blBottomSubBlock1" style="margin-right: 10px;">
    <div class="blBottomBtnBlock">Save Doc. Settings</div>
  </div>
  <div class="blBottomSubBlock1" style="margin-right: 10px;">
    <div class="blBottomBtnBlock">Upload Add Doc.</div>
  </div>
  <div class="blBottomSubBlock1" style="margin-right: 10px;">
    <div class="blBottomBtnBlock">More&nbsp;<i class="fa fa-angle-down" aria-hidden="true"></i></div>
  </div>
  <div class="blBottomSubBlock2">
    <div class="blBottomBtnMainBlock">
      <div class="blBottomBtnBlock">Save</div>
    </div>
    <div class="blBottomBtnMainBlock">
      <div class="blBottomBtnBlock">Print</div>
    </div>
  </div>
</div>

<div class="blDataRow" style="margin-top: 18px;">
  <div class="blDataBlock">
    <div class="blInputLabel">CSP Email</div>
    <div class="blInputText"><input type="text" pInputText [(ngModel)]="property"/></div>
  </div>
  <div class="blExtraSpace">&nbsp;</div>
  <div class="blDataBlock">
    <div class="blInputLabel">CC Email</div>
    <div class="blInputText"><input type="text" pInputText [(ngModel)]="property"/></div>
  </div>
  <div class="blExtraSpace">&nbsp;</div>
  <div class="blDataBlock">
    <div class="blInputLabel">Email Remarks</div>
    <div class="blInputText"><input type="text" pInputText [(ngModel)]="property"/></div>
  </div>
</div>









