}

.sideMenuBlock div.caret {
  color: inherit;
  display: inline-block;
  margin-bottom: 0;
  margin-right: 12px;
  width: 0;
  height: 0;
  overflow: hidden;
  border-style: solid;
  border-width: 6px;
  position: relative;
  border-bottom: 6px solid transparent;
  border-right: none;
  border-left-color: initial;
  border-top-color: transparent;
}

.sideMenuBlock div.caret.active {
  margin-bottom: 3px;
  margin-right: 6px;
  border-bottom: none;
  border-right: 6px solid transparent;
  border-left-color: transparent;
  border-top-color: initial;
}

.subTitleBlock {
  padding-left: 5px;
  padding-top: 8px;
  padding-bottom: 8px;

  font-weight: normal;
  font-size: 13px;
  text-decoration: none;
  display: block;
  line-height: 1;
  transition: all .3s;
}

.sideMenuBlock a {
  color: $egraymenulabel;
  font-weight: normal;
  font-size: 13px;
  text-decoration: none;
  display: block;
  line-height: 1;
  transition: all .3s;
}

