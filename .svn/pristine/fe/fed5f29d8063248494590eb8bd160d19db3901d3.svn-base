//SlideOut Css
.slideoutShowLeft {
  opacity: 1;
  left: 0px;
  overflow: auto;
  position: fixed;
  // width: 85%;
  width: 215px;
  z-index: 75;
  top: 50px;
  height: 100%;
  -webkit-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -moz-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -webkit-transition-duration: 0.3s;
  -moz-transition-duration: 0.3s;
  transition-duration: 0.3s;
}

.slideoutHideLeft {
  opacity: 1;
  left: -80%;
  overflow: auto;
  position: fixed;
  // width: 85%;
  width: 215px;
  z-index: 75;
  top: 50px;
  height: 100%;
  -webkit-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -moz-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -webkit-transition-duration: 0.3s;
  -moz-transition-duration: 0.3s;
  transition-duration: 0.3s;
}

.slideoutShowRight {
  opacity: 0.5;
  left: 85%;
  overflow: auto;
  position: fixed;
  width: 15%;
  z-index: 9999;
  top: 50px;
  height: 100%;
  -webkit-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -moz-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -webkit-transition-duration: 0.3s;
  -moz-transition-duration: 0.3s;
  transition-duration: 0.3s;
}

.slideoutHideRight {
  opacity: 0.5;
  left: -15%;
  overflow: auto;
  position: fixed;
  width: 15%;
  z-index: 99;
  top: 50px;
  height: 100%;
  -webkit-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -moz-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -webkit-transition-duration: 0.3s;
  -moz-transition-duration: 0.3s;
  transition-duration: 0.3s;
}

