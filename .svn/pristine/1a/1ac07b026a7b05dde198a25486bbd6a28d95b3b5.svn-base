@import '../../styles.scss';

//Standard Classes

.ui-inputswitch{
    border-radius: 14px;
}

.ui-inputswitch-offlabel{
    color: $egrayfieldlabel;
    font-size: 12px;
}

.pContainerSaveButtons .ui-button, .pContainerSaveButtons button.ui-button.ui-state-default, .pContainerSaveButtons .ui-button.ui-state-default{
    background: $ewhite;
    color: $emaroon;
    border: 1px solid $emaroon;
    padding: 3px !important;
}

// // Applying background color if input is disabled
input[type="text"]:disabled {
  // background-color: $egrayfield;
  background-color: $egraydisabledfield;
  // border: 1px solid $egrayborder;
  border: 1px solid $egraydisabledborder;
  color: black;
}


// Applying 100% width to inputText
.ui-inputtext {
    width: 100% !important;
}

.containerDialogHeader .ui-dialog-titlebar{
    background: $ewhite;
    border-bottom: 1px solid $egrayborder;
}

.containerDialogHeader .ui-dialog{
    overflow-y: auto !important;
    overflow-x: hidden !important;
    width: 800px !important;
    height: 625px !important;
    top: 23.5px !important;
}

.containerDialogHeader .ui-dialog .ui-resizable-se{
    width: 0px;
}

.containerPaginator .ui-paginator .ui-paginator-page.ui-state-active{
    background: $emaroon;
    border: 1px solid $emaroon;
}

.DgButton .ui-inputswitch-on{
    background: $ewhite;
    color: $egrayfield;
}

//Component Classes

