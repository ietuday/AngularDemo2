/*Media Queries*/
@media only screen
and (min-width : 266px)
{
    .containerInput{
        height: 25px;
        width: 90px;
        border-radius: 3px;
        border:1px solid $egrayborder;
        width: 100%;
    }

    .containerInputEditField{
        height: 25px;
        width: 35px;
    }
}

@media only screen
and (min-width : 404px)
{
    .containerInput{
        height: 25px;
        width: 85px;
        border-radius: 3px;
        border:1px solid $egrayborder;
        width: 100%;
    }

    .containerInputEditField{
        height: 25px;
        width: 45px;
    }
}

@media only screen
and (min-width : 480px)
{
    .containerInput{
        height: 25px;
        width: 160px;
        border-radius: 3px;
        border:1px solid $egrayborder;
        width: 100%;
    }

    .containerInputEditField{
        height: 25px;
        width: 45px;
    }
}

@media only screen
and (min-width : 768px)
{
    .containerInput{
        height: 25px;
        width: 185px;
        border-radius: 3px;
        border:1px solid $egrayborder;
    }

    .containerInputEditField{
        height: 25px;
        width: 70px;
    }
}

@media only screen
and (min-width : 950px)
{
    .containerInput{
        height: 25px;
        width: 200px;
        border-radius: 3px;
        border:1px solid $egrayborder;
    }

    .containerInputEditField{
        height: 25px;
        width: 90px;
    }
}

@media only screen
and (min-width : 1200px)
{
    .containerInput{
        height: 25px;
        width: 230px;
        border-radius: 3px;
        border:1px solid $egrayborder;
    }

    .containerInputEditField{
        height: 25px;
        width: 117px;
    }
}

@media only screen
and (min-width : 1410px)
{
    .containerInput{
        height: 25px;
        width: 250px;
        border-radius: 3px;
        border:1px solid $egrayborder;
    }

    .containerInputEditField{
        height: 25px;
        width: 150px;
    }
}
