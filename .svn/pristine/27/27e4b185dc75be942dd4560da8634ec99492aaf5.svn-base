@import '../../styles.scss';

.loginMainBlock{
  height: 672px;
  width: 100%;
  text-align: center;
  position: absolute;
  background-image: url("assets/banner.jpg");
  background-position: center top;
  padding-top: 3px;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  overflow: none !important;
}

.loginLogo{
  margin-top: 90px;
}

.loginHeader{
      font-family: sans-serif;
      text-align: center;
      margin-top: 47px;
      height: 35px;
      font-weight: bolder;
      font-size: 34px;
}

.loginInfo{
    font-family: sans-serif;
    margin-top: 23px;
    font-size: 12px;
    color: white;
    text-align: center;
    line-height: 25px;
}

.loginButton{
  margin-left: 43%;
  margin-right: 43%;
  height: 23px;
  margin-top:211px;
  background: $emaroon;
}

.signInButton {
  position: relative;
  display: inline-block;
  padding: 12px 24px;
  margin: .3em 0 1em 0;
  width: 100%;
  vertical-align: middle;
  color: #fff;
  font-size: 13px;
  height: 45px;
  line-height: 20px;
  margin-top: -3px;
  -webkit-font-smoothing: antialiased;
  text-align: center;
  letter-spacing: 1px;
  border: 0;
  cursor: pointer;
	background: $emaroon;
}

.mainBlockBefore{
  height: 0px;
  margin-top: 180px;
  display: none;
}

.mainBlockAfter{
  height: 114px;
  margin-top: 211px;
  display: block;
  // border: 1px solid black;
}

.loginMain{
  margin-left:22%;
  margin-right:22%;
  margin-top:-18px;
}

.loginUserName{
  width:25%;
  float:left;
  border:1px solid #d9d9d9;
  height:43px;
}

.group {
	position: relative;
	margin-bottom: 45px;
}

.loginInput {
	font-size: 12px;
	padding: 10px 10px 10px 5px;
	-webkit-appearance: none;
	display: block;
	background: #fafafa;
	color: $egraymenulabel;
  padding-top: 19px;
  font-weight: bold;
	width: 93%;
	border: none;
	border-radius: 0;
  border-right: 1px solid #e6e6e6;
}

/* Highlight */

.highlight {
	position: absolute;
	height: 60%;
	width: 100px;
	top: 25%;
	left: 0;
	pointer-events: none;
	opacity: 0.5;
}

.bar {
	position: relative;
	display: block;
	width: 100%;
}

.loginLabel {
	color: $egrayfieldlabel;
	font-size: 12px;
  margin-top: 5px;
	font-weight: normal;
	position: absolute;
	pointer-events: none;
	left: 5px;
	top: 10px;
	-webkit-transition:all 0.2s ease;
	transition: all 0.2s ease;
}

.loginPassword{
  width:25%;
  float:left;
  border:1px solid #d9d9d9;
  height:43px;
}

.loginDatabase{
  width:25%;
  float:left;
  background: white;
  border:1px solid #d9d9d9;
  height:43px;
}

.loginSignIn{
  width:25%;
  float:right;
  margin-top: -44px;
  height:43px;
}

.loginStatus {
  text-align: center;
  font-size: 19px;
  margin-top: 18px;
  margin-bottom: 14px;
  color:$emaroon;
  // border: 1px solid black;
}
.buttonLogin {
  position: relative;
  display: inline-block;
  padding: 12px 24px;
  margin: .3em 0 1em 0;
  width: 100%;
  vertical-align: middle;
  color: #fff;
  font-size: 12px;
  line-height: 20px;
  // -webkit-font-smoothing: antialiased;
  text-align: center;
  letter-spacing: 1px;
  background: transparent;
  border: 0;
  height: 46px;
  margin-top: -1px;
  // border-bottom: 2px solid #3160B6;">
  cursor: pointer;
  -webkit-transition:all 0.15s ease;
  transition: all 0.15s ease;
}

.buttonRed {
  background: $emaroon;
  // text-shadow: 1px 1px 0 rgba(39, 110, 204, .5);
}

.bar:before, .bar:after {
	content: '';
	height: 2px;
	width: 0;
	bottom: 1px;
	position: absolute;
	// background: #4a89dc;
	-webkit-transition:all 0.2s ease;
	transition: all 0.2s ease;
}

.bar:before { left: 50%; }

.bar:after { right: 50%; }


/* active */

.loginInput:focus ~ .bar:before, .loginInput:focus ~ .bar:after { width: 50%; }



/* active */

.loginInput:focus ~ .highlight {
	-webkit-animation: inputHighlighter 0.3s ease;
	        animation: inputHighlighter 0.3s ease;
}


.loginInput:focus { outline: none; }
/* Label */
/* active */

.loginInput:focus ~ .loginLabel, .loginInput.used ~ .loginLabel {
	top: -3px;
  -webkit-transform: scale(.75);
          transform: scale(.75); left: 0px;
  font-size: 13px;
  color: $egrayfieldlabel;
}

